{
  "name": "tippiq-oss-account-manager-api",
  "version": "0.0.1",
  "license": "GPL-3.0-only",
  "private": true,
  "dependencies": {
    "assert": "1.4.1",
    "autobind-decorator": "1.4.1",
    "aws-sdk": "2.78.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "3.5.0",
    "body-parser": "1.17.2",
    "bookshelf": "0.10.3",
    "bookshelf-cascade-delete": "2.0.1",
    "connect-ensure-login": "0.1.1",
    "connect-roles": "3.1.2",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.2",
    "debugnyan": "2.0.1",
    "dotenv": "4.0.0",
    "email-templates": "2.6.0",
    "es6-error": "4.0.2",
    "express": "4.15.3",
    "express-bunyan-logger": "1.3.2",
    "knex": "0.13.0",
    "lodash": "4.17.4",
    "nodemailer": "4.0.1",
    "nodemailer-ses-transport": "1.5.1",
    "nunjucks": "3.0.1",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "passport-totp": "0.0.2",
    "pg": "7.12.1"
  },
  "devDependencies": {
    "autoprefixer": "7.1.0",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react-app": "^3.0.0",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.23.0",
    "bunyan": "1.8.10",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-react": "7.0.1",
    "jest": "20.0.4",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "supertest": "3.0.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon --trace-warnings --verbose --exec babel-node --watch src src/server.js",
    "build": "NODE_ENV=production babel --copy-files --source-maps inline --out-dir build src",
    "lint": "eslint src",
    "test": "jest",
    "start": "NODE_ENV=production node build/server.js",
    "cleanup": "rm -rf node_modules build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost:3017",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "airbnb",
      "plugin:jest/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "jest"
    ],
    "env": {
      "jest/globals": true
    },
    "rules": {
      "arrow-parens": "off",
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
